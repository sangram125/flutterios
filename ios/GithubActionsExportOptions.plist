    <string>MM7U35V4FZ</string> <!-- Your team ID -->
    <key>thinning</key>
    <string>&lt;none&gt;</string>
    <key>signingCertificate</key>
    <string>iOS Distribution</string> <!-- iOS Distribution certificate -->
