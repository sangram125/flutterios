<plist version="1.0">
<dict>
    <key>compileBitcode</key>
    <false/>
    <key>destination</key>
    <string>export</string>
    <key>method</key>
    <string>app-store</string> <!-- App Store distribution -->
    <key>signingStyle</key>
    <string>manual</string>
    <key>provisioningProfiles</key>
    <dict>
        <key>tv.dorplay.companion</key> <!-- Bundle identifier -->
        <string>cicd-new</string> <!-- Provisioning profile name -->
