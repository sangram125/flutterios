<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>compileBitcode</key>
    <true/>
    <key>method</key>
    <string>app-store</string>
    <key>provisioningProfiles</key>
    <dict>
        <key>tv.dorplay.companion</key> <!-- Replace with your app's Bundle Identifier -->
        <string>cicd-new</string> <!-- Replace with your Provisioning Profile Name -->
    </dict>
    <key>signingCertificate</key>
    <string>iOS Distribution</string> <!-- Certificate type for App Store deployment -->
    <key>signingStyle</key>
    <string>manual</string>
    <key>stripSwiftSymbols</key>
    <true/>
    <key>teamID</key>
    <string>MM7U35V4FZ</string> <!-- Replace with your Apple Developer Team ID -->
    <key>thinning</key>
    <string>&lt;none&gt;</string>
</dict>
</plist>
